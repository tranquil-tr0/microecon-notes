{
  "name": "microecon-website",
  "version": "1.0.0",
  "description": "AP Microeconomics website built from Obsidian markdown",
  "main": "dist/index.html",
  "scripts": {
    "build": "tsc && bun run dist/build.js",
    "dev": "concurrently \"bun run tsc --watch\" \"bun run server.ts\"",
    "watch-ts": "tsc --watch",
    "start": "bun run server.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ap-microeconomics",
    "education",
    "website",
    "obsidian",
    "markdown"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "@types/jsdom": "^21.0.0",
    "@types/lz-string": "^1.5.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^18.15.0",
    "concurrently": "^8.2.0",
    "serve": "^14.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@excalidraw/utils": "^0.1.3-test32",
    "excalidraw-to-svg": "^3.1.0",
    "jsdom": "^23.0.0",
    "lz-string": "^1.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-ins": "^4.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-obsidian-callouts": "^0.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
